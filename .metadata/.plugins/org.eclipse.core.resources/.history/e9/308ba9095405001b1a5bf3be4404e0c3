var apiclientCadena = (function(){
    var equipoBackEnd = "nginx:8000"
    return{
    	addCadenas(cadena){
            return $.ajax({
                url:  equipoBackEnd+"/balanceador",
                type: "POST",
                data: '{"name":"'+cadena+'"}',
                contentType: "application/json"});
        }
    };
    
}());
